(fun 
	freqsy
	(line
		(seq 0.05 0.5)
		(rv 10 30)))

(fun index
	(
		list
		(seq 0 5 10 15 20 25)
		(seq 0 2 4 8 10 12)
	)
	(t
		(rv 0 2)
		(rv 13 33)))




(clone 
(midi-note
	(/ 1.0 (mtof (scaleAC freqsy (st 100) (st -60))))
 	(+ (t (ch 40 47 54 61) (rv 1 3)) 
 		(seq 0 5 10 15 20 25))
 	(st 0.3)
	(st 90)
	) 2)
