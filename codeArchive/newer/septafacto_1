(~ pit

		(reset 
			(walk 60 (hold 
				(+ (seq (ch 0 0 0 0 0 0 7) -12 12) (hold (seq 0 2 -7) (ch 7 13 15)))
				(ch 2 3 1 1)))
			(st 60)
			(st 4))
		)

(~ dura (seq 0.5 0.1 0.1))

(~ timr (seq 0.15 0.1))

(~ velo (loop (ch 20 30 40 30 20 0 120 80 100) (st 3) (st 3)))

