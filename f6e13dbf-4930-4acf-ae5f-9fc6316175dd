(sync 1)

(fun a
	(hold
	(seq 9 12 19 14 16 24)
	(ch 7 5 12)))

(fun b
	(hold
		(seq 0 2 -2)
		(ch 7 12)))

(sci2
	casper
	(st 0.14)
	:freq (mtof (+ 
		(+ 
		(seq b (seq 5 5 5 5 5 5 5 5) 7) 
		(hold (seq b 5 (seq a a a 0 2) 7) (seq 3 5 4 3 5 2 2 3 4 5 3)))
		48))
	:duration (weigths ((1.0 100) (10.0 5)))
	:amp (seq 0.1 0.05 0.1 0.05 0.1))
