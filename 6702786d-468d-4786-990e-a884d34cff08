(# tab1
	(fillf 128 -1 1))

(~ writeOne
	(write tab1 (rv -1 1) (count 128)))

(schedule
	(~ writeOne) 
	(/
		1.0
		(line (seq 1 1000) (ch .1 .3 .5 1 3 10))))

(fun stepper
    (hold
		(ch -1 1)
		(mtof
			(line
				(seq 0 100)
				(ch .1 .3 .5 1 3 10)))))

(# possibleSteps
	(list 1. 2 3 4 5 11 23 41 56 72 111 1001))

(fun a 
	(line
		(seq 0 (table-cap possibleSteps))))

(~ stepSize
	(index
		possibleSteps
		(rv a a)))

(step-gen
	(list-walk tab1 stepper)
	(~ stepSize)
	:pan (rvfi -1 1))